{
  "name": "fems",
  "version": "1.0.0",
  "description": "Student registration system with Jest testing",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:simple": "jest src/__tests__/StudentService.test.js src/__tests__/SimpleDataDriven.test.js",
    "test:manual": "node manual-test-json.js",
    "verify": "node verify-project.js"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js"
    ],
    "transform": {},
    "extensionsToTreatAsEsm": [],
    "transformIgnorePatterns": [
      "node_modules/(?!(uuid)/)"
    ]
  },
  "keywords": [
    "nodejs",
    "jest",
    "testing",
    "student-registration"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "jest": "^30.2.0",
    "nodemon": "^3.1.11"
  }
}
